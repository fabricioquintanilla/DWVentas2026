            <component
              refId="Package\Llenar DimProducto\Leer tablas de productos"
              componentClassID="Microsoft.ManagedComponentHost"
              contactInfo="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server.;Microsoft Corporation; Microsoft SQL Server; © Microsoft Corporation; All Rights Reserved; http://www.microsoft.com/sql/support;4"
              description="Consumes data from SQL Server, OLE DB, ODBC, or Oracle, using the corresponding .NET Framework data provider. Use a Transact-SQL statement to define the result set. For example, extract data from SQL Server with the .NET Framework data provider for SQL Server."
              name="Leer tablas de productos"
              usesDispositions="true"
              version="4">
              <properties>
                <property
                  dataType="System.String"
                  description="Specifies the SQL statement used by the component to extract data."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">select p.ProductID,
p.Name as Nombre,
p.ProductNumber,
ISNULL(CONVERT(VARCHAR(50),
CASE P.Color
WHEN 'Black' THEN 'Negro'
WHEN 'Blue' THEN 'Azul'
WHEN 'Grey' THEN 'Gris'
WHEN 'Multi' THEN 'Multiple'
WHEN 'Red' THEN 'Rojo'
WHEN 'Silver' THEN 'Plateado'
WHEN 'Silver/Black' THEN 'Plateado Oscuro'
WHEN 'White' THEN 'Blanco'
WHEN 'Yellow' THEN 'Amarillo'
ELSE P.Color END
),'Color No Definido')
as Color,
ISNULL(CONVERT(VARCHAR(50),CASE p.ProductLine
WHEN 'R' THEN 'Carretera'
WHEN 'M' THEN 'Montana'
WHEN 'T' THEN 'Turismo'
WHEN 'S' THEN 'Estandar' ELSE P.ProductLine END),
'Linea de Producto No Definida') as LineaProducto,
ISNULL(CONVERT(VARCHAR(50),CASE p.Class
WHEN 'H' THEN 'Alto'
WHEN 'M' THEN 'Mediano'
WHEN 'L' THEN 'Pequeno' ELSE P.Class END),
'Clase No Definida') AS Clase,
ISNULL(CONVERT(VARCHAR(50),CASE p.Style
WHEN 'W' THEN 'Mujeres'
WHEN 'M' THEN 'Hombres'
WHEN 'U' THEN 'Universal' ELSE p.Style END),
'Estilo No Definido') AS Estilo,
p.ListPrice,
ISNULL(ps.Name, 'Subcategoria No Definida') as Subcategoria,
ISNULL(pc.Name, 'Categoria No Definida') as Categoria,
ISNULL(pm.Name, 'Modelo No Definido') as Modelo
from production.Product p
left outer join Production.ProductSubcategory ps
on ps.ProductSubcategoryID=p.ProductSubcategoryID
left outer join Production.ProductCategory pc
on pc.ProductCategoryID=ps.ProductCategoryID
left outer join Production.ProductModel pm
on pm.ProductModelID=p.ProductModelID</property>
